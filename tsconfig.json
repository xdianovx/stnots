{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/contacts/index.jsx",
    "src/ui/PinLink/PinLink.jsx",
    "src/screens/Fillial/Fillial.jsx",
    "src/pages/team/index.jsx",
    "src/pages/team/index.jsx",
    "src/components/TeamCard/TeamCard.jsx",
    "src/components/TeamCard/TeamCard.jsx",
    "src/components/TeamScrollSection/TeamScrollSection.jsx",
    "src/components/TeamScrollSection/TeamScrollSection.jsx",
    "src/components/TeamScrollSection/TeamScrollCard.jsx",
    "src/components/TeamScrollSection/TeamScrollCard.jsx",
    "src/components/Transition/Transition.jsx",
    "src/components/Transition/Transition.jsx",
    "src/pages/projects/[slug]/index.jsx",
    "src/pages/projects/[slug]/index.jsx",
    "src/pages/projects/index.jsx",
    "src/pages/projects/index.jsx",
    "src/screens/NextProject/NextProject.jsx",
    "src/screens/NextProject/NextProject.jsx",
    "src/pages/vacancy/index.jsx",
    "src/pages/vacancy/index.jsx",
    "src/screens/VacancySection/VacancySection.jsx",
    "src/screens/VacancySection/VacancySection.jsx",
    "src/screens/VacancySection/VacancyItem.jsx",
    "src/screens/VacancySection/VacancyItem.jsx"
  ],
  "exclude": ["node_modules"]
}
